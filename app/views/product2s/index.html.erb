<p id="notice"><%= notice %></p>

<h1>Product2s</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product2s.each do |product2| %>
      <tr>
        <td><%= product2.image %></td>
        <td><%= product2.title %></td>
        <td><%= product2.url %></td>
        <td><%= link_to 'Show', product2 %></td>
        <td><%= link_to 'Edit', edit_product2_path(product2) %></td>
        <td><%= link_to 'Destroy', product2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product2', new_product2_path %>
