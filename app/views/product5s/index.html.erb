<p id="notice"><%= notice %></p>

<h1>Product5s</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product5s.each do |product5| %>
      <tr>
        <td><%= product5.image %></td>
        <td><%= product5.title %></td>
        <td><%= product5.url %></td>
        <td><%= link_to 'Show', product5 %></td>
        <td><%= link_to 'Edit', edit_product5_path(product5) %></td>
        <td><%= link_to 'Destroy', product5, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product5', new_product5_path %>
