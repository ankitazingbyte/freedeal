<div class="container" >
  <div class= "row">
    <!-- <div id="map" class="col-md-12" style="width:985px;height:400px;background:none !important"></div> -->
    <div id="map_wrapper">
      <div id="map_canvas" class="mapping"></div>
    </div>
    <br>
    <div class= "col-md-6">  
      <h1>Contact Informations</h1> 
        <% @contacts.each do |contact| %>
          <p><div class= "col-md-2"><%= image_tag "address_logo.jpeg", width: "40px" %></div>Address<br><%= contact.address %></p><br>
          <p><div class= "col-md-2"><%= image_tag "email_logo.png", width: "40px" %></div>Email<br><%= contact.email %></p><br>
          <p><div class= "col-md-2"><%= image_tag "phone.jpeg", width: "40px" %></div>Phone<br><%= contact.phone %></p>
        <% end %>
    </div>
    <div class= "col-md-6">
      <h1>Get In Touch</h1> 
      <%= render "messages/form" %>
    </div>
  </div>
</div>
